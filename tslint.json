{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"ordered-imports": true,
		"object-literal-sort-keys": false,
		"await-promise": true,
		"curly": [
			true,
			"ignore-same-line"
		],
		"no-eval": true,
		"prefer-const": true,
		"no-return-await": true,
		"cyclomatic-complexity": [
			true,
			20
		],
		"eofline": true
	},
	"rulesDirectory": []
}